class GameOfLife {
    function void run() {
        var GameField gameField;
        let gameField = GameField.new(30, 30);

        while (~Controller.quit()) {
            do Editor.edit(gameField);
            do Player.play(gameField);
            do Sys.wait(500);
        }
        
        do gameField.reDraw();
        do gameField.dispose();
        return;
    }
}